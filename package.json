{
  "name": "recipeapp",
  "private": true,
  "scripts": {
    "prebuild": "npx expo prebuild --clean",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "doctor": "npx expo-doctor@latest",
    "start": "npx expo start",
    "start-production": "expo start --no-dev --minify",
    "update-build": "eas update --branch production --clear-cache",
    "submit-build": "eas build --profile production --platform ios --clear-cache --auto-submit",
    "submit-build-local": "eas build --profile production --platform ios --local",
    "postinstall": "husky install && patch-package",
    "version": "bun run prebuild && git add .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noemit",
    "lint:translations": "eslint ./src/translations/ --fix --ext .json  ",
    "gen-supabase": "supabase gen types --lang=typescript --project-id cfzwkhnvjvfjfgazjlzh > src/database/types/database-generated.types.ts",
    "prettier": "prettier --config .prettierrc.js 'src/**/*.ts' --write",
    "update:stagining": "eas update --channel staging",
    "upload-sourcemaps": "npx sentry-expo-upload-sourcemaps dist",
    "update-expo": "bun add expo && npx expo install --fix",
    "devtools": "npx react-devtools",
    "upgrade-maestro": "curl -fsSL \"https://get.maestro.mobile.dev\" | bash",
    "run-e2e": "maestro test .maestro"
  },
